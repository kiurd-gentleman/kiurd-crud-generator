{
  "name": "krimt/kiurd-crud-generator",
  "description": "A Laravel package to generate CRUD API from a JSON file",
  "license": "MIT",
  "keywords": [
    "laravel",
    "crud",
    "api",
    "generator",
    "json"
  ],
  "homepage": "https://github.com/kiurd-gentleman/kiurd-crud-generator",
  "support": {
    "issues": "https://github.com/kiurd-gentleman/kiurd-crud-generator/issues",
    "source": "https://github.com/kiurd-gentleman/kiurd-crud-generator"
  },
  "autoload": {
    "psr-4": {
      "Krimt\\ApiFirstCrudPackage\\": "src/"
    }
  },
  "authors": [
    {
      "name": "kiurd-gentelman",
      "email": "65729806+kiurd-gentleman@users.noreply.github.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.44"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Krimt\\ApiFirstCrudPackage\\Providers\\ApiCrudServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}